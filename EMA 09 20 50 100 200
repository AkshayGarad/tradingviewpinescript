// EMA Indicator
study("Exponential Moving Averages", overlay=true)

// Define the EMA periods
periods = [9, 20, 50, 100, 200]

// Plot the EMAs
for period in periods
    ema = ema(close, period)
    plot(ema, color=color(period), linewidth=2)

// Add a legend
legend(title="EMA", position=position.top)
